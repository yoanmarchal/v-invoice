{
  "name": "v-invoice",
  "version": "0.3.0",
  "private": true,
  "main": "dist/v-invoice.umd.js",
  "module": "dist/v-invoice.mjs",
  "exports": {
    ".": {
      "import": "./dist/v-invoice.mjs",
      "require": "./dist/v-invoice.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "vite build",
    "lint": "oxlint --vue-plugin --vitest-plugin --import-plugin --tsconfig=../jsconfig.json --ignore-path=.oxlintignore src tests",
    "format": "oxfmt --config=.oxfmtrc.json src tests",
    "format:check": "oxfmt --check --config=.oxfmtrc.json src tests",
    "test:unit": "vitest --config vitest.config.mjs"
  },
  "dependencies": {
    "vue": "^3.5.11"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitest/ui": "^1.6.1",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.22",
    "babel-jest": "^29.7.0",
    "jsdom": "^27.3.0",
    "oxfmt": "^0.17.0",
    "oxlint": "^1.32.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "vite": "^7.2.7",
    "vitest": "^1.6.1"
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/bundlers/vue@2.6.15"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  }
}
